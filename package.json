{
    "name": "vscode-pashmak",
    "displayName": "Pashmak programming language",
    "description": "Pashmak programming language support for VSCode",
    "version": "0.0.1",
    "publisher": "parsampsh",
    "repository": "https://github.com/pashmaklang/vscode-pashmak",
    "icon": "data/pashmak-logo.png",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pashmak",
            "aliases": ["Pashmak", "pashmak"],
            "extensions": [".pashm", ".pashmt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pashmak",
            "scopeName": "source.pashmak",
            "path": "./syntaxes/pashmak.tmLanguage.json"
        }]
    }
}
